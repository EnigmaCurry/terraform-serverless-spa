{
  "name": "terraform-serverless-spa",
  "version": "0.1.0",
  "private": true,
  "config": {
    "public_html_s3_bucket": "",
    "aws_cloudfront_distribution": ""
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "check-config": "node scripts/check-config.js",
    "deploy": "npm run check-config && npm run build && aws s3 sync dist/ s3://$npm_package_config_public_html_s3_bucket --delete && npm run cache-invalidate",
    "cache-invalidate": "npm run check-config && aws cloudfront create-invalidation --distribution-id $npm_package_config_aws_cloudfront_distribution --paths /index.html | node scripts/cloudfront-wait-for-invalidation.js",
    "test": "vue-cli-service test",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open",
    "config": "node scripts/npmrc-config.js"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^2.0.3",
    "axios": "^0.18.0",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.13",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.6",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.6",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "ts-jest": "^22.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
